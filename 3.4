#include<iostream>
#include<string>
using namespace std;
int camelCase(string s){
    int count=1;
    for(int i=0;i<s.size();++i){
        if(s[i]>='A' && s[i]<='Z'){
            count++;
        }
    }
    return count;
    
}
int main(){
    string s;
    cin>>s;
    int resul=camelCase(s);
    cout<<resul<<endl;
    return 0;
}
