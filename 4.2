#include <iostream>
#include <vector>
#include <unordered_map>

using namespace std;

int equalizeArray(vector<int> arr) {
   
    unordered_map<int, int> frequency;
    for (int num : arr) {
        frequency[num]++;
    }
    
    
    int maxFrequency = 0;
    for (auto it : frequency) {
        maxFrequency = max(maxFrequency, it.second);
    }
    
   
    return arr.size() - maxFrequency;
}

int main() {
    int n;
    cin >> n;
    
    vector<int> arr(n);
    for (int i = 0; i < n; ++i) {
        cin >> arr[i];
    }

    int result = equalizeArray(arr);
    cout << result << endl;

    return 0;
}
