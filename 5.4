#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n;
    cin>>n;
    vector<int> v(n);
    for(int i=0;i<n;++i){
        cin>>v[i];
        
    }
    int q;
    cin>>q;
    while(q--){
        int x;
        cin>>x;
        auto lb=lower_bound(v.begin(),v.end(),x);
        if(lb!=v.end() && *lb==x){
            cout<<"Yes"<<" "<<lb-v.begin()+1<<endl;
        }else{
            cout<<"No"<<" "<<lb-v.begin()+1<<endl;
        }
    }
    
    return 0;
}
