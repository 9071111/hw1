#include<iostream>
#include<string>
using namespace std;
void print(const string& s){
    string even;
    string odd;
    for(int i=0;i<s.size();++i){
        if(i%2==0){
            even+=s[i];
        }else{
            odd+=s[i];
        }
    }
    cout<<even<<" "<<odd<<endl;
}
int main(){
    int t;
    cin>>t;
    cin.ignore();
    for(int i=0;i<t;++i){
        string s;
        getline(cin,s);
        print(s);
    }
    
    cout<<endl;
    return 0;
}
