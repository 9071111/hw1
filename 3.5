#include<iostream>
#include<string>
using namespace std;
void hackerrankInString(string s){
    string cur="hackerrank";
    int st=0;
    for(int i=0;i<s.size() && st<cur.size();++i ){
        if(s[i]==cur[st]){
            st++;
        }
    }
    if(st==cur.size()){
        cout<<"YES"<<endl;
    }else {
        cout<<"NO"<<endl;
    }
}   
int main(){
    int t;
    cin>>t;
    for(int i=0;i<t;++i){
        string s;
        
        cin>>s;
        hackerrankInString(s);
        
    }
    return 0;
    
}
